{
  "jsonElementVariable": {
    "prefix": "jev",
    "body": "\"${1:key}\": ${2:value}, $0",
    "description": "JSON Element Variable"
  },
  "jsonElementString": {
    "prefix": "jes",
    "body": "\"${1:key}\": \"${2:value}\", $0",
    "description": "JSON Element String"
  },
  "jsonElementNumber": {
    "prefix": "jen",
    "body": "\"${1:key}\": ${2:number}, $0",
    "description": "JSON Element Number"
  },
  "jsonElementBoolean": {
    "prefix": "jeb",
    "body": "\"${1:key}\": ${2|true,false|}, $0",
    "description": "JSON Element Boolean"
  },
  "jsonElementObject": {
    "prefix": "jeo",
    "body": "\"${1:key}\": {${2:element}, $3}, $0",
    "description": "JSON Element Object"
  },
  "jsonElementObjectMultiline": {
    "prefix": "jeom",
    "body": ["\"${1:key}\": {", "\t${2:element}, $3", "}, $0"],
    "description": "Object Element Object"
  },
  "jsonElementArray": {
    "prefix": "jea",
    "body": "\"${1:key}\": [${2:value}, $3], $0",
    "description": "JSON Element Array"
  },
  "jsonElementArrayMultiline": {
    "prefix": "jeam",
    "body": ["${1:key}: [", "\t${2:value}, $3", "], $0"],
    "description": "Object Element Object"
  }
}
