{
  "constantVariable": {
    "prefix": "cv",
    "body": "const ${1:name} = $0",
    "description": "Constant Variable"
  },
  "constantVariableType": {
    "prefix": "cvt",
    "body": "const ${1:name}: ${2|string,number,boolean,custom|} = $0",
    "description": "Constant Variable Type"
  },
  "constantVariableMultipleType": {
    "prefix": "cvm",
    "body": "const ${1:name}: (${2|string,number,boolean,custom|} | ${3|string,number,boolean,custom|}) = $0",
    "description": "Constant Variable Type"
  },
  "constantString": {
    "prefix": "cs",
    "body": "const ${1:name}: string = '$0'",
    "description": "Constant String"
  },
  "constantNumber": {
    "prefix": "cn",
    "body": "const ${1:name}: number = ${0:0}",
    "description": "Constant Number"
  },
  "constantBoolean": {
    "prefix": "cb",
    "body": "const ${1:name}: boolean = ${2|true,false|}${0}",
    "description": "Constant Boolean"
  },
  "constantObject": {
    "prefix": "co",
    "body": "const ${1:name} = {$0}",
    "description": "Constant Object"
  },
  "constantObjectInterface": {
    "prefix": "coi",
    "body": "const ${1:name}: ${3:Interface} = {$0}",
    "description": "Constant Object Interface"
  },
  "constantArray": {
    "prefix": "ca",
    "body": "const ${1:name} = [$0]",
    "description": "Constant Array"
  },
  "constantArrayType": {
    "prefix": "cat",
    "body": "const ${1:name}: ${2|string,number,boolean,custom|}[] = [$0]",
    "description": "Constant Type Array"
  },
  "constantArrayMultiType": {
    "prefix": "cam",
    "body": "const ${1:name}: (${2|string,number,boolean,custom|} | ${3|string,number,boolean,custom|})[] = [$0]",
    "description": "Constant Multi Type Array"
  },
  "constantArrowFunction": {
    "prefix": "caf",
    "body": ["const ${1:name} = ($2) => {", "\t$0", "}"],
    "description": "Constant Arrow function (implicit return type)"
  },
  "constantAsyncArrowFunction": {
    "prefix": "caaf",
    "body": ["const ${1:name} = async ($2) => {", "\t$0", "}"],
    "description": "Constant Async Arrow function"
  },
  "constantArrowFunctionType": {
    "prefix": "caft",
    "body": [
      "const ${1:name} = ($2) : ${3|string,number,boolean,custom|} => {",
      "\t$0",
      "}"
    ],
    "description": "Constant Arrow function (explicit return type)"
  },
  "constantAsyncArrowFunctionType": {
    "prefix": "caaft",
    "body": [
      "const ${1:name} = async ($2) : ${3|string,number,boolean,custom|} => {",
      "\t$0",
      "}"
    ],
    "description": "Constant Async Arrow function (explicit return type)"
  }
}
