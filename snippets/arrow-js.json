{
  "arrowFunction": {
    "prefix": "af",
    "body": "() => $0",
    "description": "Arrow function (implicit return)"
  },
  "arrowFunctionArguments": {
    "prefix": "afa",
    "body": "(${1:arg}) => $0",
    "description": "Arrow function (implicit return) with arguments"
  },
  "arrowFunctionExplicit": {
    "prefix": "afe",
    "body": ["() => {", "  \treturn $0", "};"],
    "description": "Arrow function (explicit return)"
  },
  "arrowFunctionExplicitArguments": {
    "prefix": "afea",
    "body": ["(${1:arg}) => {", "  \treturn {$0:arg}", "};"],
    "description": "Arrow function (explicit return) with arguments"
  },
  "arrowFunctionParentesis": {
    "prefix": "afp",
    "body": ["() => {", "  \t( {$0:arg} )", "};"],
    "description": "Arrow function (explicit without return)"
  },
  "arrowFunctionEParentesisArguments": {
    "prefix": "afpa",
    "body": ["(${1:arg}) => {", "  \t( {$0:arg} )", "};"],
    "description": "Arrow function (explicit without return) with arguments"
  },
  "arrowFunctionFilterEqual": {
    "prefix": "affeq",
    "body": "(${1:element}) => ${1:element}.${2:prop} === ${0:value}",
    "description": "Arrow function (implicit return) filter elements"
  },
  "arrowFunctionFilterNotEqual": {
    "prefix": "affne",
    "body": "(${1:element}) => ${1:element}.${2:prop} !== ${0:value}",
    "description": "Arrow function (implicit return) filter elements"
  }
}
